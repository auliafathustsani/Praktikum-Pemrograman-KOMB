#include <iostream>
using namespace std;

int main() {
    int a, b, c, bilangan_terbesar;
    
    cout << "Bilangan pertama: ";
    cin >> a;
    cout << "Bilangan kedua: ";
    cin >> b;
    cout << "Bilangan ketiga: ";
    cin >> c;
    
    switch (a > b) {
      case 1 :
       switch(a > c){
            case 1 :
               cout << "Bilangan terbesar = " << a << endl;
               break;
            case 0 :
               cout << "Bilangan terbesar = " << c << endl;
               break;
        }
        break;
      case 0 :
        switch(b > c){
            case 1 :
               cout << "Bilangan terbesar = " << b << endl;
               break;
            case 0 :
               cout << "Bilangan terbesar = " << c << endl;
               break;
        }
        break;
    }
    return 0;
}
